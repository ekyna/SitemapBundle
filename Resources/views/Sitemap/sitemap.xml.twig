<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"> {# TODO check version #}
{% spaceless %}
{% for provider in providers %}
    {% for entry in provider.entries -%}
    <url>
        <loc>{{ entry.location }}</loc>
        <lastmod>{{ entry.lastmod|date('Y-m-d\TH:i:sP') }}{# TODO gmdate(DATE_W3C) #}</lastmod>
        <changefreq>{{ entry.changefreq }}</changefreq>
        <priority>{{ entry.priority }}</priority>
    </url>
    {% endfor %}
{% endfor %}
{% if groups is defined %}
{% for group in groups %}
    {# TODO  {{ path('ekyna_sitemap_group', {'group', group}) }}  #}
{% endfor %}
{% endif %}
{% endspaceless %}
</urlset>