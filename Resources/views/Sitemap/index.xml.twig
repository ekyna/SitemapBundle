{% spaceless %}
<?xml version='1.0' encoding='UTF-8'?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for sitemap, lastmod in sitemaps %}
    <sitemap>
        <loc>{{ url('ekyna_sitemap_sitemap', {'sitemap': sitemap}) }}</loc>
        <lastmod>{{ lastmod|date('c') }}</lastmod>
    </sitemap>
{% endfor %}
</sitemapindex>
{% endspaceless %}